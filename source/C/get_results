#!/bin/bash
while IFS='' read -r line || [[ -n "$line" ]]; do
	points=`echo $line | cut -d' ' -f93`;
	printf "%03d $line\n" $points;
done < "results" | sort -r > sorted_results
while IFS='' read -r line || [[ -n "$line" ]]; do
	points=`echo $line | cut -d' ' -f93`;
	printf "%03d $line\n" $points;
done < "results11" | sort -r > sorted_results11
